   "source": [
    "import numpy as np\n",
    "import sys;sys.path.append("../../../")\n",
    "from scipy.stats import multivariate_normal as mvn\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "from mcmcpy.samplers.metropolis_hastings import SingleChainMetropolisHastings\n",
    "from mcmcpy.proposals.normal import AdaptiveNormal"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "a66f8a87",
   "metadata": {},
   "source": [
    "## Initialize x, density, proposal, and single chain metropolis hastings (SMH) object ##"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "8500fda5",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Initialize first sample and mean and covariance of proposal distribution\n",
    "x = mvn(mean=np.zeros(2), cov=np.eye(2)).rvs(1)\n",
    "    \n",
    "mu1 = np.zeros(2)\n",
    "cov1 = np.eye(2)\n",
    "mu2 = np.array([2,2])\n",
    "var2_1 = pow(0.3,2)\n",
    "var2_2 = pow(0.3,2)\n",
    "rho2 = 0.8\n",
    "cov2 = np.array([[var2_1, rho2*np.sqrt(var2_1)*np.sqrt(var2_2)],\n",
    "                 [rho2*np.sqrt(var2_1)*np.sqrt(var2_2), var2_2]])\n",
    "dist1 = mvn(mean=mu1, cov=cov1)\n",
    "dist2 = mvn(mean=mu2, cov=cov2)\n",
    "\n",
    "density = lambda X: (2/3) * dist1.pdf(X) + (1/3)*dist2.pdf(X)\n",
    "proposal = AdaptiveNormal(var=np.eye(x.shape[0]))\n",
    "SMH = SingleChainMetropolisHastings(density, proposal)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "8f482b3e",
   "metadata": {},
   "source": [
    "## Sample from the target distribution ##"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "45ffd3f8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Accepted Rate: 0.23687631236876314\n"
     ]
    }
   ],
   "source": [
    "SMH.sample(x, steps=10000)\n",
    "print(f\"Accepted Rate: {SMH.chain.accepted_state_count/SMH.steps}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "3aea4bcf",
   "metadata": {},
   "outputs": [
    {
     "data": {
